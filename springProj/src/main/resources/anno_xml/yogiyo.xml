<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://www.springframework.org/schema/beans"
xmlns:p="http://www.springframework.org/schema/p"
xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans
https://www.springframework.org/schema/beans/spring-beans-4.3.xsd
http://www.springframework.org/schema/context
https://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<!-- 술판 -->
	<bean name="yg1" class="anno_p.Yogiyo" p:name="술판" />

	<!-- 술 -->
	<bean name="dr1" class="anno_p.Drink"
	p:name="소주" p:price="4000" p:bottle="1" />
	<bean name="dr2" class="anno_p.Drink"
	p:name="맥주" p:price="5000" p:bottle="1" />
	<bean name="dr3" class="anno_p.Drink"
	p:name="막걸리" p:price="4500" p:bottle="1" />
	<bean name="dr4" class="anno_p.Drink"
	p:name="하이볼" p:price="7000" p:bottle="1" />
	
	<!-- 안주 -->
	<bean name="ds1" class="anno_p.Dish"
	p:name="돼지김치구이" p:price="18000" p:cnt="1" />
	<bean name="ds2" class="anno_p.Dish"
	p:name="맥앤치즈감자튀김" p:price="135000" p:cnt="1" />
	<bean name="ds3" class="anno_p.Dish"
	p:name="치즈감자전" p:price="145000" p:cnt="1" />
	<bean name="ds4" class="anno_p.Dish"
	p:name="치킨가라아게" p:price="145000" p:cnt="1" />
	
	
	<context:annotation-config/>
</beans>
