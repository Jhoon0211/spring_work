package aop_p.webToon;

import org.aspectj.lang.JoinPoint;
import org.springframework.stereotype.Component;

@Component
public class Circle {

	// 원 넓이
	public int circleArea(int r, double cirA) {
		int r1=4;
		cirA = r1*r1*Math.PI; 
		return cirA;
		
	}
	
	// 원 둘레
	public int circleRound(int r, int cirR) {
		int r2=5;
		cirR = (int)(r2*2*Math.PI);
		return cirR;	
	}
}
