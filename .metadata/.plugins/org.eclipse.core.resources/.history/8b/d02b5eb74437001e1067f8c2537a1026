package di_p;

public class phone {
	String name;
	Battery bat;
	Display dis;
	Camera cam;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setBat(Battery bat) {
		this.bat = bat;
	}
	public void setDis(Display dis) {
		this.dis = dis;
	}
	public void setCam(Camera cam) {
		this.cam = cam;
	}
	
	public phone(String name, Battery bat, Display dis, Camera cam) {
		super();
		this.name = name;
		this.bat = bat;
		this.dis = dis;
		this.cam = cam;
	}
	
	@Override
	public String toString() {
		return "phone [name=" + name + ", cam=" + cam + "]";
	}

	
	
}

class Battery{
	String name, power;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPower() {
		return power;
	}

	public void setPower(String power) {
		this.power = power;
	}

	@Override
	public String toString() {
		return "Battery [name=" + name + ", power=" + power + "]";
	}
	
	
}
class Display{
	String name, size;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getSize() {
		return size;
	}

	public void setSize(String size) {
		this.size = size;
	}

	@Override
	public String toString() {
		return "Display [name=" + name + ", size=" + size + "]";
	}
	
	
}	

class Kamera{
	String name, pixel;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPixel() {
		return pixel;
	}

	public void setPixel(String pixel) {
		this.pixel = pixel;
	}


	
}