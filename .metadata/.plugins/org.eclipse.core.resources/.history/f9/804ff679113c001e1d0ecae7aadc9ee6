package aop_p.webToon;

import org.aspectj.lang.JoinPoint;
import org.springframework.stereotype.Component;

@Component
public class Circle {

	// 원 넓이
	public int circleArea(int r) {
		int r=4;
		cirround = (int) (r*r*Math.PI); 
		return cirarea;
		
	}
	
	// 원 둘레
	public int circleRound() {
		
		return cirround;	
	}
}
