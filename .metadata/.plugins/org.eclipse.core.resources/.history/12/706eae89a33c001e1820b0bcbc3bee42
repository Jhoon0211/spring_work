package aop_p.classes;

import org.springframework.stereotype.Component;

@Component
public class AdultStudent {
	
	public int as1(int kor, int eng, int tot) {
		// 리턴 값
		tot = kor+eng;
		System.out.println("국어 점수 : " + kor + "영어 점수" + eng + "평균");
		return tot;
	}
	
	public int as2(int kor, int eng, int tot) {
		// 리턴 값
		tot = kor+eng;
		System.out.println("국어 점수 : " + kor);
		return tot;
	}
	
	public int as3(int kor, int eng, int tot) {
		// 리턴 값
		tot = kor+eng;
		System.out.println("국어 점수 : " + kor);
		return tot;
	}
}
