package di_p;

public class Grade {
	Red red;
	Blue blue;
	Yellow yellow;
	
	public Red getRed() {
		return red;
	}
	public void setRed(Red red) {
		this.red = red;
	}
	public Blue getBlue() {
		return blue;
	}
	public void setBlue(Blue blue) {
		this.blue = blue;
	}
	public Yellow getYellow() {
		return yellow;
	}
	public void setYellow(Yellow yellow) {
		this.yellow = yellow;
	}
	
	@Override
	public String toString() {
		return "Grade [red=" + red + ", blue=" + blue + ", yellow=" + yellow + "]";
	}
	
	
}

class Red{
	String name;
	int r1, r2, r3, r4, r5;
	public void setName(String name) {
		this.name = name;
	}
	public void setR1(int r1) {
		this.r1 = r1;
	}
	public void setR2(int r2) {
		this.r2 = r2;
	}
	public void setR3(int r3) {
		this.r3 = r3;
	}
	public void setR4(int r4) {
		this.r4 = r4;
	}
	public void setR5(int r5) {
		this.r5 = r5;
	}
	@Override
	public String toString() {
		return "Red [name=" + name + ", r1=" + r1 + ", r2=" + r2 + ", r3=" + r3 + ", r4=" + r4 + ", r5=" + r5 + "]";
	}
	
	
}

class Blue{
	String name;
	int b1, b2, b3, b4, b5;


	public void setName(String name) {
		this.name = name;
	}


	public void setB1(int b1) {
		this.b1 = b1;
	}


	public void setB2(int b2) {
		this.b2 = b2;
	}


	public void setB3(int b3) {
		this.b3 = b3;
	}


	public void setB4(int b4) {
		this.b4 = b4;
	}


	public void setB5(int b5) {
		this.b5 = b5;
	}


	@Override
	public String toString() {
		return "Blue [name=" + name + ", b1=" + b1 + ", b2=" + b2 + ", b3=" + b3 + ", b4=" + b4 + ", b5=" + b5 + "]";
	}
	
}

class Yellow{
	String name;
	int y1, y2, y3, y4, y5;


	
	public void setName(String name) {
		this.name = name;
	}



	public void setY1(int y1) {
		this.y1 = y1;
	}



	public void setY2(int y2) {
		this.y2 = y2;
	}



	public void setY3(int y3) {
		this.y3 = y3;
	}



	public void setY4(int y4) {
		this.y4 = y4;
	}



	public void setY5(int y5) {
		this.y5 = y5;
	}



	@Override
	public String toString() {
		return "Yellow [name=" + name + ", y1=" + y1 + ", y2=" + y2 + ", y3=" + y3 + ", y4=" + y4 + ", y5=" + y5 + "]";
	}
	
	
}