package anno_p.com;

import org.springframework.stereotype.Component;

@Component
public class CafeMenu {
	String name;
	Americano americano;
	Cafelatte cafelatte;
	Caramel caramel;
	
	@Override
	public String toString() {
		return "CafeMenu [name=" + name + ", americano=" + americano + ", cafelatte=" + cafelatte + ", caramel="
				+ caramel + "]";
	}
		
}

@Component("ame")
class Americano{
	String name, coffeebean;
	int water, price;
		
}

@Component("latte")
class Cafelatte{
	String name, coffeebean;
	int water, price;
	
}

@Component("maki")
class Caramel{
	String name, coffeebean;
	int piece, water, price;
	
}

