package di_p;

import java.util.Arrays;

public class Exam {
	String name;
	Student student;
	Korea korea;
	English english;
	Math math;
	public void setName(String name) {
		this.name = name;
	}
	public void setStudent(Student student) {
		this.student = student;
	}
	public void setKorea(Korea korea) {
		this.korea = korea;
	}
	public void setEnglish(English english) {
		this.english = english;
	}
	public void setMath(Math math) {
		this.math = math;
	}
	
	public Exam(String name, Student student, Korea korea, English english, Math math) {
		super();
		this.name = name;
		this.student = student;
		this.korea = korea;
		this.english = english;
		this.math = math;
	}
	
	@Override
	public String toString() {
		return "Exam [name=" + name + ", student=" + student + ", korea=" + korea + ", english=" + english + ", math="
				+ math + "]";
	}
		

}
class Student{
	String name;

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "Student [name=" + name + "]";
	}
	
	
}


class Korea{
	String name;
	int [] korScr;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setScr(int[] korScr) {
		this.korScr = korScr;
	}
	
	@Override
	public String toString() {
		return "Korea [name=" + name + ", korScr=" + korScr + "]";
	}

}

class English{
	String name;
	int [] engScr;
	
	public void setName(String name) {
		this.name = name;
	}
	
	public void setEng(int[] engScr) {
		this.engScr = engScr;
	}

	
	@Override
	public String toString() {
		return "English [name=" + name + ", engScr=" + engScr + "]";
	}

	
}

class Math{
	String name;
	int [] mathScr;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setScr(int[] mathScr) {
		this.mathScr = mathScr;
	}
	
	@Override
	public String toString() {
		return "Math [name=" + name + ", mathScr=" + mathScr + "]";
	}
	
}