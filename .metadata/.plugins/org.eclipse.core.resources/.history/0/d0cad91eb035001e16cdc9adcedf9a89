package di_p;

public class Bicycle {
	String name;
	ThreeBicycle tb;
	PocariBicycle pb;
	MTBBicycle mb;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public ThreeBicycle getTb() {
		return tb;
	}
	public void setTb(ThreeBicycle tb) {
		this.tb = tb;
	}
	public PocariBicycle getPb() {
		return pb;
	}
	public void setPb(PocariBicycle pb) {
		this.pb = pb;
	}
	public MTBBicycle getMb() {
		return mb;
	}
	public void setMb(MTBBicycle mb) {
		this.mb = mb;
	}
	
	@Override
	public String toString() {
		return "Bicycle [name=" + name + ", tb=" + tb + ", pb=" + pb + ", mb=" + mb + "]";
	}	
	
}
