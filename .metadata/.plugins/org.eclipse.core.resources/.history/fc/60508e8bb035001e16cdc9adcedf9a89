package di_p;

public class MTBBicycle {
	String handle, blanket;
	int wheel;
	
	/*
	public Three() {
		System.out.println("세발자전거 기본생성자");
	}
	
	public Pocari() {
		
	}
	
	public MTB() {
		
	}
	*/
	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}

	public MTBBicycle() {
		System.out.println("MTBBicycle 기본생성자");
	}
	
	public String getHandle() {
		return handle;
	}


	public void setHandle(String handle) {
		System.out.println(handle);
		this.handle = handle;
	}


	public String getBlanket() {
		return blanket;
	}


	public void setBlanket(String blanket) {
		System.out.println(blanket);
		this.blanket = blanket;
	}


	public int getWheel() {
		return wheel;
	}


	public void setWheel(int wheel) {
		System.out.println("MTBBicycle setWheel():" + wheel);
		this.wheel = wheel;
	}


	@Override
	public String toString() {
		return "Bicycle [handle=" + handle + ", blanket=" + blanket + ", wheel=" + wheel + "]";
	}

	
	
}
