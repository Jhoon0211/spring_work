package aop_p.webToon;

import org.aspectj.lang.JoinPoint;
import org.springframework.stereotype.Component;

@Component
public class Rectagle {
	
	// 직사각형 넓이
	public int rectagleArea(int w, int h, int recA) {
		recA = (w*h)*(w*h);
		return recA;
		
	}
	
	// 직사각형 둘레
	public int rectagleRound(int w, int h, int recR) {
		recR = (w+h)*2;
		return recR;
		
	}
}
