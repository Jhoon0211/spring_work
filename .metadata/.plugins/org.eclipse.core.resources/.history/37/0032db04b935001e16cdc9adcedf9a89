package di_p;

public class Bicycle {
	String three, pocari, mtb;
	Wheel tire;
	Handle hand;
	Basket basket;
	
	public String getThree() {
		return three;
	}
	public void setThree(String three) {
		this.three = three;
	}
	public String getPocari() {
		return pocari;
	}
	public void setPocari(String pocari) {
		this.pocari = pocari;
	}
	public String getMtb() {
		return mtb;
	}
	public void setMtb(String mtb) {
		this.mtb = mtb;
	}
	public Wheel getTire() {
		return tire;
	}
	public void setTire(Wheel tire) {
		this.tire = tire;
	}
	public Handle getHand() {
		return hand;
	}
	public void setHand(Handle hand) {
		this.hand = hand;
	}
	public Basket getbasket() {
		return basket;
	}
	public void setbasket(basket blank) {
		this.basket = basket;
	}
	
	@Override
	public String toString() {
		return "Bicycle [three=" + three + ", pocari=" + pocari + ", mtb=" + mtb + ", tire=" + tire + ", hand=" + hand
				+ ", basket=" + basket + "]";
	}

	
	
}
