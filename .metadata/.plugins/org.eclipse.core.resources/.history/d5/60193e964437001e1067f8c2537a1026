package di_p;

public class phone {
	String name;
	Battery bat;
	Display dis;
	Camera cam;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setBat(Battery bat) {
		this.bat = bat;
	}
	public void setDis(Display dis) {
		this.dis = dis;
	}
	public void setCam(Camera cam) {
		this.cam = cam;
	}
	
	public phone(String name, Battery bat, Display dis, Camera cam) {
		super();
		this.name = name;
		this.bat = bat;
		this.dis = dis;
		this.cam = cam;
	}
	
	@Override
	public String toString() {
		return "phone [name=" + name + ", cam=" + cam + "]";
	}

	
	
}

class Battery{
	String name, power;

}
class Display{
	String name, size;
}	
class Camera{
	String name, pixel;
}