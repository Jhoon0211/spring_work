package di_p;

import java.util.Arrays;

public class Exam {
	String name;
	Korea ks;
	English es;
	Math ms;
	//Total tot;
	//Average avg;
	public void setName(String name) {
		this.name = name;
	}
	public void setKs(Korea ks) {
		this.ks = ks;
	}
	public void setEs(English es) {
		this.es = es;
	}
	public void setMs(Math ms) {
		this.ms = ms;
	}
	public Exam(String name, Korea ks, English es, Math ms) {
		super();
		this.name = name;
		this.ks = ks;
		this.es = es;
		this.ms = ms;
	}
	@Override
	public String toString() {
		return "Exam [name=" + name + ", ks=" + ks + ", es=" + es + ", ms=" + ms + "]";
	}
	
	// 학생 점수 배열
	//int [] arr, arr1, arr2, arr3;
	// 총점, 평균
	//int tot, avg;

	

}

class Korea{
	String name;
	int [] korScr;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setScr(int[] korScr) {
		this.korScr = korScr;
	}
	
	@Override
	public String toString() {
		return "Korea [name=" + name + ", korScr=" + korScr + "]";
	}

}

class English{
	String name;
	int [] engScr;
	
	public void setName(String name) {
		this.name = name;
	}
	
	public void setEng(int[] engScr) {
		this.engScr = engScr;
	}

	
	@Override
	public String toString() {
		return "English [name=" + name + ", engScr=" + engScr + "]";
	}

	
}

class Math{
	String name;
	int [] mathScr;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setScr(int[] mathScr) {
		this.mathScr = mathScr;
	}
	
	@Override
	public String toString() {
		return "Math [name=" + name + ", mathScr=" + mathScr + "]";
	}
	
}