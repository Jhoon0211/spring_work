package di_p;

import java.util.Arrays;

public class Student {
	String name;
	int[] arr1, arr2, arr3;
	int tot, avg;
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int[] getArr1() {
		return arr1;
	}
	public void setArr1(int[] arr1) {
		this.arr1 = arr1;
	}
	public int[] getArr2() {
		return arr2;
	}
	public void setArr2(int[] arr2) {
		this.arr2 = arr2;
	}
	public int[] getArr3() {
		return arr3;
	}
	public void setArr3(int[] arr3) {
		this.arr3 = arr3;
	}
	public int getTot() {
		return tot;
	}
	public void setTot(int tot) {
		this.tot = tot;
	}
	public int getAvg() {
		return avg;
	}
	public void setAvg(int avg) {
		this.avg = avg;
	}

	public Student(String name, int[] arr1, int[] arr2, int[] arr3, int tot, int avg) {
		super();
		this.name = name;
		this.arr1 = arr1;
		this.arr2 = arr2;
		this.arr3 = arr3;
		this.tot = tot;
		this.avg = avg;
	}
	
	@Override
	public String toString() {
		return "Student [name=" + name + ", arr1=" + Arrays.toString(arr1) + ", arr2=" + Arrays.toString(arr2)
				+ ", arr3=" + Arrays.toString(arr3) + ", tot=" + tot + ", avg=" + avg + "]";
	}

	
	
}