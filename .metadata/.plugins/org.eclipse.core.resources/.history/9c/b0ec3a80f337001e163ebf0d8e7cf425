package di_p;

public class Grade {
	Red red;
	Blue blue;
	Yellow yellow;
	
	public Red getRed() {
		return red;
	}
	public void setRed(Red red) {
		this.red = red;
	}
	public Blue getBlue() {
		return blue;
	}
	public void setBlue(Blue blue) {
		this.blue = blue;
	}
	public Yellow getYellow() {
		return yellow;
	}
	public void setYellow(Yellow yellow) {
		this.yellow = yellow;
	}
	
	@Override
	public String toString() {
		return "Grade [red=" + red + ", blue=" + blue + ", yellow=" + yellow + "]";
	}
	
	
}

class Red{
	String name;
	int r1, r2, r3, r4, r5;
}

class Blue{
	String name;
	int b1, b2, b3, b4, b5;
	
}

class Yellow{
	String name;
	int y1, y2, y3, y4, y5;
}