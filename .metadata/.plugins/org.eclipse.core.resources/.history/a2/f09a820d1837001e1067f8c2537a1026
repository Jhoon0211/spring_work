package di_p;

public class CoffeeShop {
	
	Coffee order_am() {
		System.out.println("order_am 실행");
		return new Coffee("아메리카노", 1400);
	}
}

class Coffee{
	String name;
	int price;
	
	// 설정자
	public Coffee(String name, int price) {
		super();
		this.name = name;
		this.price = price;
	}

	// 재정의 - 잠재적 오류 제거
	@Override
	public String toString() {
		return "Coffee [name=" + name + ", price=" + price + "]";
	}
	
	
	
	
}