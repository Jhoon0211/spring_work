package di_p;

public class Menu {
	String name, main, side;
	int price;
	
	
	public Menu(String name, String main, String side, int price) {
		super();
		this.name = name;
		this.main = main;
		this.side = side;
		this.price = price;
	}

	@Override
	public String toString() {
		return "Menu [name=" + name + ", main=" + main + ", side=" + side + ", price=" + price + "]";
	}
	
}
