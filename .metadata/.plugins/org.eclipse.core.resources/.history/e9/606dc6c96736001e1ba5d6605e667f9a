package di_p;

public class Exam {
	String name;
	Korea kor;
	English eng;
	Math mat;
	Total tot;
	Average avg;
	
	// 생성자 선언
	public Exam(String name, Korea kor, English eng, Math mat, Total tot, Average avg) {
		super();
		this.name = name;
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
		this.tot = tot;
		this.avg = avg;
		
		
	}

	@Override
	public String toString() {
		return "Exam [name=" + name + ", kor=" + kor + ", eng=" + eng + ", mat=" + mat + ", tot=" + tot + ", avg=" + avg
				+ "]";
	}
	
}

class Korea{
	String name;
	int scr;
	
	public Korea(String name, int scr) {
		super();
		this.name = name;
		this.scr = scr;
	}
}

class English{
	String name;
	int scr;
	
	public English(String name, int scr) {
		super();
		this.name = name;
		this.scr = scr;
	}
	
}

class Math{
	String name;
	int scr;
	
	public Math(String name, int scr) {
		super();
		this.name = name;
		this.scr = scr;
	}
}

class Total{
	String name;
	int tot;
	
	public Total(String name, int tot) {
		super();
		this.name = name;
		this.tot = tot;
	}
}

class Average{
	String name;
	int avg;
	
	public Average(String name, int avg) {
		super();
		this.name = name;
		this.avg = avg;
	}
	
}