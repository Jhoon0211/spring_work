package di_p;

public class phone {
	String name;
	Battery bat;
	Display dis;
	Camera cam;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setBat(Battery bat) {
		this.bat = bat;
	}
	public void setDis(Display dis) {
		this.dis = dis;
	}
	public void setCam(Camera cam) {
		this.cam = cam;
	}

	
}
