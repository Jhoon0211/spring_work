package di_p;

public class Exam {
	String name;
	Korea kor;
	English eng;
	Math mat;
	//Total tot;
	//Average avg;
	
	// 학생 점수 배열
	int [] arr;
	int tot, avg;
	
	public int[] getArr() {
		return arr;
	}

	public void setArr(int[] arr) {
		this.arr = arr;
	}

	// 생성자 선언
	public Exam(String name, Korea kor, English eng, Math mat, Total tot, Average avg) {
		super();
		this.name = name;
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
		this.tot = tot;
		this.avg = avg;
	}

	@Override
	public String toString() {
		return "Exam [name=" + name + ", kor=" + kor + ", eng=" + eng + ", mat=" + mat + ", tot=" + tot + ", avg=" + avg
				+ "]";
	}
	
}

class Korea{
	String name;
	int scr;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setScr(int scr) {
		this.scr = scr;
	}
	
	@Override
	public String toString() {
		return "Korea [name=" + name + ", scr=" + scr + "]";
	}

}

class English{
	String name;
	int scr;
	

	
}

class Math{
	String name;
	int scr;
	

}
}