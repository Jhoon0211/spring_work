package di_p;

import java.util.Arrays;

public class Exam {
	String name;
	Student student;
	Korea korea;
	English english;
	Math math;
	
	

	

}
class Student{
	String name;
}


class Korea{
	String name;
	int [] korScr;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setScr(int[] korScr) {
		this.korScr = korScr;
	}
	
	@Override
	public String toString() {
		return "Korea [name=" + name + ", korScr=" + korScr + "]";
	}

}

class English{
	String name;
	int [] engScr;
	
	public void setName(String name) {
		this.name = name;
	}
	
	public void setEng(int[] engScr) {
		this.engScr = engScr;
	}

	
	@Override
	public String toString() {
		return "English [name=" + name + ", engScr=" + engScr + "]";
	}

	
}

class Math{
	String name;
	int [] mathScr;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setScr(int[] mathScr) {
		this.mathScr = mathScr;
	}
	
	@Override
	public String toString() {
		return "Math [name=" + name + ", mathScr=" + mathScr + "]";
	}
	
}