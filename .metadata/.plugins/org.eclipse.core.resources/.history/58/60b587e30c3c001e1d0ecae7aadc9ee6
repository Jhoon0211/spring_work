package aop_p.webToon;

import org.springframework.stereotype.Component;

@Component
public class Round {

	// 원 넓이
	public int circleRound(int cirarea) {
		return cirarea;
		
	}
	
	// 직사각형 넓이
	public int rectagleRound(int recarea) {
		return recarea;
		
	}
	
	// 정사각형 넓이
	public int squareRound(int squarea) {
		return squarea;
		
	}
}
