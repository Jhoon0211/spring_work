https://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
<bean class="di_p.Rest" name="menu">
		<!-- (되는거) order_am의 결과를 co로 바꿔치기 -->
		<lookup-method name="comeon_menu" bean="brk" /> 
		
		<!-- (안되는거) -->
		<!-- void는 적용되지 않음 return이 없어서 -->
		<lookup-method name="order_void" bean="co" /> 
		
		<!-- <lookup-method name="order_tea" bean="co" /> order_tea의 결과를 co로 바꿔치기 ==> return 자료형이 맞지 않음 -->
		
		<!-- private는 lookup-method 적용되지 않음 (의미 없음) -->
		<lookup-method name="order_private" bean="co" />
		
		<!-- final은 상수라 대입이 안 됨 -->
		<lookup-method name="order_final" bean="co" />
		
		<lookup-method name="order_static" bean="co" />
		
		<lookup-method name="order_piano" bean="co" />

		<lookup-method name="order_hunter" bean="co" />
		
		<lookup-method name="order_latte" bean="co" />
	</bean>
	
	<bean class="di_p.Menu" name="brk">
			<constructor-arg value="아침식단"/> <!-- name -->
			<constructor-arg value="에그토스트"/> <!-- main -->
			<constructor-arg value="소세지"/> <!-- side -->
			<constructor-arg value="5000"/> <!-- price -->
	</bean>
	<bean class="di_p.Menu" name="lun">
			<constructor-arg value="점심식단"/> <!-- name -->
			<constructor-arg value="제육볶음"/> <!-- main -->
			<constructor-arg value="소야볶"/> <!-- side -->
			<constructor-arg value="6000"/> <!-- price -->
	</bean>
	<bean class="di_p.Menu" name="din">
